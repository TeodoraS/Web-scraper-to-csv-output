import random


all_rolls = []

def roll_dice(n):
    rolls =[]
    while n > 0:
        roll = random.randrange(1, 7, 1)
        n = n - roll
        rolls.append(roll)
    print rolls
    all_rolls.append(rolls)


def verify():
    for element in all_rolls:
        if sum(element) != 20:
            remove(element)

roll_dice(20)

verify()

print all_rolls

