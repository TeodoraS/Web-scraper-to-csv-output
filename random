import random


all_rolls = []

game_nr = int(raw_input("Insert the number of games:\t"))

def roll_dice(n):
    rolls =[]
    while n > 0:
        roll = random.randrange(1, 7, 1)
        n = n - roll
        rolls.append(roll)
    print rolls
    all_rolls.append(rolls)


def verify_dice():
    for element in all_rolls:
        if sum(element) != 20:
            remove(element)
        else:
            pass
        return all_rolls

while len(all_rolls) < game_nr :
    roll_dice(20)
    verify_dice()
    continue
    
